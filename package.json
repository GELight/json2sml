{
  "name": "@gelight/json2sml",
  "private": false,
  "version": "1.0.0",
  "description": "Node package to convert JSON to SML",
  "main": "dist/index.js",
  "unpkg": "releases/json2sml.min.js",
  "jsdelivr": "releases/json2sml.min.js",
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GELight/json2sml"
  },
  "keywords": [
    "ReliableTxt",
    "WhitespaceSV",
    "SimpleML",
    "SML",
    "Simple",
    "Markup",
    "Language",
    "Converter"
  ],
  "author": "Mario Linz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GELight/json2sml/issues"
  },
  "homepage": "https://github.com/GELight/json2sml#readme",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' npm run build",
    "build": "tsc && node dist/index.js",
    "bundle": "tsc && webpack --config webpack.config.js",
    "eslint": "eslint . --ext .ts",
    "test:unit": "tsc && jest --coverage",
    "release": "./build-release.sh"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.9",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "auto-changelog": "^2.3.0",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0"
  },
  "files": [
    "dist",
    "releases"
  ],
  "dependencies": {
    "@gelight/sml": "^0.3.3"
  }
}
